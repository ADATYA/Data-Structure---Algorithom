#include <stdio.h>

int main()
{
    int a[]={12,23,54,-12.5};  // array use korle last a ; dite hoy
    
    int searchItem =23;
    int i;
    for(i = 0;i<4;i++){
        if(a[i] == searchItem){
            printf("Search item is found : %d\n",i);
        return 0;
            
        }
        
    }
    printf("Item is not found.\n");
    return 0;
}
